@{
    Layout = "~/Views/AppsViews/Membership/_MembershipLayout.cshtml";
}
@section SearchListTopMenu{
    <input type="text" data-ng-model="searchText.Name" />
    <a data-ng-click="displayCreateForm()"><i class="fa fa-plus"></i> Add Company</a>
}
@section DetailsTopMenu{

}
@section SearchList{
    <ul>
        <li class="searchListItem" data-ng-class="{ active: $index == selectedIndex && (state == DetailsPanelStates.Info || state == DetailsPanelStates.Edit)}" data-ng-repeat="company in companies | filter:searchText">
            <a data-ng-click="displayDetails($index)">{{company.Name}}</a><i class="fa fa-times extMenuChoice" data-ng-click="deleteCompany($index)"></i><i class="fa fa-users extMenuChoice" data-ng-click="displayCompanyUsers($index)"></i><i class="fa fa-pencil extMenuChoice" data-ng-click="displayEditForm($index)"></i>
        </li>
    </ul>
}
