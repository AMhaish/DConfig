                        <div class="card-body">
                            <div class="form-group">
                                <input class="form-control" name="id" type="text" data-ng-model="currentCompany.Id" value="" readonly="readonly" />
                                <label for="id">Id</label>
                            </div>
                            <div class="form-group">
                                <input class="form-control" name="name" type="text" data-ng-model="currentCompany.Name" value="" readonly="readonly" />
                                <label for="name">Name</label>
                            </div>
                            <div class="form-group">
                                <textarea class="form-control" name="address" data-ng-model="currentCompany.Address" value="" readonly="readonly" />
                                <label for="name">Address</label>
                            </div>
                            <div class="form-group">
                                <input class="form-control" name="city" type="text" data-ng-model="currentCompany.City" value="" readonly="readonly" />
                                <label for="name">City</label>
                            </div>
                            <div class="form-group">
                                <input class="form-control" name="country" type="text" data-ng-model="currentCompany.Country" value="" readonly="readonly" />
                                <label for="name">Country</label>
                            </div>
                            <div class="form-group">
